</audio>

<video id="media" height="480" value="1">
    <source src="~/Content/Video/Video.mp4" />
</video>
<br />
<button type="button" id="play">Play</button>

<script>
    $(document).ready(function () {
        new App.Home.VideoPageController();

    });
</script>
